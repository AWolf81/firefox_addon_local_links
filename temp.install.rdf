<?xml version="1.0" encoding="UTF-8"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>local-link-addon@awolf81.github.com</em:id>
    <em:name>Local link addon</em:name>
    <em:description>Make local links clickable in Firefox. Any link with "file:///" will open in window explorer.
    </em:description>

    <em:version>0.0.1</em:version>
    <em:type>2</em:type>
    
    <em:creator>Alexander Wolf</em:creator>
    <em:homepageURL>http://github.com/awolf81/link-firefox-addon</em:homepageURL>

    <em:targetApplication>
        <Description>
            <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!--Firefox-->
            <em:minVersion>38.0a1</em:minVersion>
            <em:maxVersion>46.*</em:maxVersion>
        </Description>
    </em:targetApplication>
  </Description>
</RDF>